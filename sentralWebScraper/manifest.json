{
    "manifest_version" : 2,
    "name": "Sentral Webscraper",
    "description" : "To be a timetable",
    "version" : "1.0.0",
    "icons": {
        "16" : "/icons/image16x16.png",
        "48" : "/icons/image48x48.png",
        "128": "/icons/image128x128.png"

    },
    "browser_action:"{
        "default_popup": "index.html"    
},
    "content_scripts": [
        {
        "matches": ["<all_urls>"],
        "js": ["Content.js"]
        }
    ] 

}
